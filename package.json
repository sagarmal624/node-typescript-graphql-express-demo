{
  "name": "apollo-graphql-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
    "prebuild": "rimraf dist",
    "run-local": "nodemon --watch src -e ts,graphql --exec yarn start:local",
    "copy-to-dist": "copyfiles --up 1  config/*.js src/graphql/typedefs/* dist",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run dev",
    "compile-sass": "node-sass assets/scss/operation-portal.scss assets/css/operation-portal.css",
    "minify-sass": "node-sass assets/scss/operation-portal.scss assets/css/operation-portal.min.css --output-style compressed",
    "map-sass": "node-sass assets/scss/operation-portal.scss assets/css/operation-portal.css --source-map true",
    "postinstall": "husky install",
    "dev": "nodemon --watch src -e ts,graphql --exec yarn start:dev",
    "format": "prettier --write README.md 'src/**/*.ts'",
    "build": "tsc && yarn copy-to-dist",
    "start": "node dist/index.js",
    "start:local": "cross-env ENV=local npm run start",
    "start:ci": "cross-env ENV=ci npm run start",
    "start:debug": "ndb ts-node src/index.ts",
    "start:dev": "cross-env ENV=dev npm run start",
    "start:qa": "cross-env ENV=qa npm run start",
    "start:alpha": "cross-env ENV=alpha npm run start",
    "start:staging": "cross-env ENV=staging npm run start",
    "start:preprod": "cross-env ENV=preprod npm run start",
    "start:perftest": "cross-env ENV=perftest npm run start",
    "start:test": "cross-env ENV=test next start",
    "start:prod": "cross-env ENV=prod next start",
    "test": "jest --config --passWithNoTests ./config/test/jest.config.js",
    "test:watch": "npm run test --watch",
    "test:coverage": "npm run test --coverage",
    "test:updateSnapshot": "npm run test -- --updateSnapshot",
    "test:noc": "jest  --config ./config/test/jest.config.js --no-coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@zeit/next-css": "1.0.1",
    "@zeit/next-sass": "1.0.1",
    "apollo-datasource": "^0.6.1",
    "apollo-server": "^2.25.1",
    "apollo-server-express": "^2.25.1",
    "aws-sdk": "^2.929.0",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "fs-capacitor": "^6.2.0",
    "graphql": "^14.7.0",
    "graphql-constraint-directive": "^2.1.1",
    "graphql-redis-subscriptions": "^2.1.1",
    "graphql-tools": "^7.0.5",
    "install": "^0.13.0",
    "jest": "^26.6.3",
    "jest-sonar-reporter": "^2.0.0",
    "merge-graphql-schemas": "^1.6.2-3705b28.0",
    "next": "^10.2.0",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^7.0.1",
    "next-images": "^1.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "request": "^2.88.2",
    "winston": "^3.3.3",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@types/graphql": "14.2.3",
    "@types/node": "8.10.51",
    "copyfiles": "^2.1.1",
    "husky": "^6.0.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "resolutions": {
    "fs-capacitor": "^6.2.0"
  }
}
